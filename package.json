{
  "name": "lib-tools",
  "version": "0.0.1-alpha.0",
  "author": "DagonMetric",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DagonMetric/lib-tools.git"
  },
  "bugs": {
    "url": "https://github.com/DagonMetric/lib-tools/issues"
  },
  "homepage": "https://github.com/DagonMetric/lib-tools",
  "engines": {
    "node": ">=10.9.0"
  },
  "bin": {
    "lib": "./bin/lib"
  },
  "experimental": true,
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "eslint": "eslint",
    "tsc": "tsc",
    "rimraf": "rimraf",
    "lint": "eslint -c .eslintrc.json --ext .js,.ts .",
    "compile": "tsc --project tsconfig.build.json",
    "build": "rimraf -- dist && tsc --project tsconfig.build.json && node tools/generate-schemas.js && node tools/copy-files.js && node dist/bin/lib --version",
    "show-help": "node -r source-map-support/register dist/bin/lib --help",
    "samples:copy-clean-demo:build": "node -r source-map-support/register ./dist/bin/lib build --config=samples/copy-clean-demo/lib.json --version=1.0.0",
    "samples:bootstrap-css-demo:build": "node -r source-map-support/register ./dist/bin/lib build --config=samples/bootstrap-css-demo/lib.json --version=1.0.0"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "ajv": "^6.12.2",
    "ajv-keywords": "^3.5.0",
    "autoprefixer": "^9.8.4",
    "builtins": "^3.0.1",
    "clean-css": "^4.2.3",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "html-minifier": "^4.0.0",
    "loader-utils": "^2.0.0",
    "magic-string": "^0.25.7",
    "minimatch": "^3.0.4",
    "postcss": "^7.0.32",
    "resolve": "^1.17.0",
    "rollup": "^2.18.2",
    "supports-color": "^7.1.0",
    "uglify-js": "^3.10.0",
    "update-notifier": "^4.1.0",
    "webpack": "^4.43.0",
    "yargs": "^15.4.0"
  },
  "devDependencies": {
    "@types/autoprefixer": "^9.7.2",
    "@types/clean-css": "^4.2.2",
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.2",
    "@types/html-minifier": "^4.0.0",
    "@types/loader-utils": "^2.0.1",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^14.0.14",
    "@types/resolve": "^1.17.1",
    "@types/sass": "^1.16.0",
    "@types/supports-color": "^5.3.0",
    "@types/uglify-js": "^3.9.2",
    "@types/webpack": "^4.41.18",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^28.6.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "typescript": "~3.9.6",
    "typescript-json-schema": "^0.42.0"
  },
  "optionalDependencies": {
    "sass": "^1.26.9"
  }
}
