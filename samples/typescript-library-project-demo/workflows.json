{
  "$schema": "../../dist/schemas/schema.json",
  "projects": {
    "full-config-demo": {
      "root": "./",
      "actions": {
        "build": {
          "outputPath": "dist/full-config-demo/typescript-library-project-demo",
          "scriptTranspilation": {
            "entries": [
              {
                "declaration": true,
                "target": "ES2015",
                "outDir": "esm2015"
              },
              {
                "declaration": false,
                "target": "es5",
                "outDir": "esm5"
              }
            ],
            "tsConfig": "tsconfig.build.json",
            "addToPackageJson": true,
            "entry": "public_api.js"
          }
        }
      }
    },
    "minimal-config-demo": {
      "root": "./",
      "actions": {
        "build": {
          "scriptTranspilation": true
        }
      }
    }
  }
}
