{
  "$schema": "../../dist/schemas/schema.json",
  "projects": {
    "typescript-library-project-full-config-demo": {
      "root": "./",
      "actions": {
        "build": {
          "outputPath": "dist/full-config-demo",
          "script": {
            "compilations": [
              {
                "declaration": true,
                "target": "ES2015",
                "outDir": "esm2015",
                "esBundle": {
                  "outputFile": "fesm2015/demo.js"
                }
              },
              {
                "declaration": false,
                "target": "es5",
                "outDir": "esm5",
                "esBundle": true,
                "umdBundle": {
                  "outputFile": "bundles/demo.umd.js"
                }
              }
            ],
            "tsConfig": "tsconfig.build.json",
            "addToPackageJson": true,
            "entry": "public_api.ts",
            "moduleName": "demo",
            "peerDependenciesAsExternals": true,
            "dependenciesAsExternals": true,
            "externals": {
              "tslib": "tslib"
            }
          }
        }
      }
    }
  }
}
